{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "no-unused-vars": "off",
    "indent": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "new-cap": "off",
    "quotes": "off",
    "operator-linebreak": "off",
    "switch-colon-spacing": "off"
  },
  "extends": [
    "eslint:recommended",
   "google"
  ]
}